{
  "name": "@qlever-llc/fl-sync",
  "version": "1.4.14",
  "description": "A Trellis microservice to sync Food Logiq data to a Trellis cloud",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "watch:test": "tsc -b --watch test",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "type": "module",
  "author": "Sam Noel <sam@qlever.io>",
  "license": "Apache-2.0",
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^5.1.1",
    "@oada/jobs": "^4.5.1",
    "@oada/lib-prom": "^3.8.0",
    "@oada/list-lib": "4.3.0",
    "@oada/poll": "^1.0.4",
    "@oada/types": "^3.5.3",
    "@overleaf/o-error": "^3.4.0",
    "@types/json-pointer": "^1.0.34",
    "axios": "^1.6.8",
    "bluebird": "^3.7.2",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "cron": "^3.1.7",
    "csvjson": "^5.1.0",
    "date-fns": "^3.6.0",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "es-main": "^1.3.0",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.0",
    "fuse.js": "^7.0.0",
    "js-sha256": "^0.11.0",
    "json-pointer": "^0.6.2",
    "json-ptr": "^3.1.1",
    "jsonpath": "^1.1.1",
    "jszip": "^3.10.1",
    "ksuid": "^3.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "mssql": "^10.0.2",
    "serialize-error": "^11.0.3",
    "sha256": "^0.2.0",
    "tslib": "^2.6.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bluebird": "^3.5.42",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/cron": "^2.4.0",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.4",
    "@types/md5": "^2.3.5",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@yarnpkg/sdks": "^3.1.2",
    "ava": "^6.1.3",
    "bluebird": "^3.7.2",
    "c8": "^9.1.0",
    "chai": "^5.1.1",
    "chai-as-promised": "^7.1.2",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "eslint-formatter-pretty": "^6.0.1",
