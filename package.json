{
  "name": "@qlever-llc/fl-sync",
  "version": "1.2.12",
  "description": "A Trellis microservice to sync Food Logiq data to a Trellis cloud",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc -b",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "type": "module",
  "author": "Sam Noel <sam@qlever.io>",
  "license": "Apache-2.0",
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^4.1.1",
    "@oada/jobs": "^4.0.0",
    "@oada/list-lib": "3.0.8",
    "@oada/poll": "^0.0.3",
    "@oada/types": "^2.0.1",
    "@overleaf/o-error": "^3.4.0",
    "@types/json-pointer": "^1.0.31",
    "adm-zip": "^0.5.9",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "convict": "^6.2.3",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "csvjson": "^5.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "es-main": "^1.0.2",
    "esm": "^3.2.25",
    "http": "^0.0.1-security",
    "http2": "^3.3.7",
    "js-sha256": "^0.9.0",
    "json-pointer": "^0.6.2",
    "jszip": "^3.10.0",
    "ksuid": "^3.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "momentjs": "^2.0.0",
    "mssql": "^8.1.2",
    "node-stream-zip": "^1.15.0",
    "nodemon": "^2.0.16",
    "sha256": "^0.2.0",
    "string-sanitizer": "^2.0.2",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34",
    "ws": "^8.7.0",
    "zip-stream": "^4.1.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/bluebird": "^3.5.36",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/mssql": "^7.1.5",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.6",
    "@yarnpkg/sdks": "^3.0.0-rc.6",
    "ava": "4.0.0-rc.1",
    "bluebird": "^3.7.2",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-config-xo-typescript": "^0.51.1",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
