{
  "name": "@qlever-llc/fl-sync",
  "version": "2.0.0",
  "description": "A Trellis microservice to sync Food Logiq data to a Trellis cloud",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "watch:test": "tsc -b --watch test",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "startjq": "node ${INSPECT:-} --enable-source-maps dist/index.js | tee test.log | pino-pretty -clt",
    "log": "tail -f test.log | jq --stream 'select(.lib != \"list-lib\")'",
    "test": "yarn build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "type": "module",
  "author": "Sam Noel <sam@qlever.io>",
  "license": "Apache-2.0",
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^6.0.0",
    "@oada/jobs": "^5.0.0",
    "@oada/lib-prom": "^4.0.1",
    "@oada/list-lib": "6.1.0",
    "@oada/pino-debug": "^4.0.1",
    "@oada/poll": "^3.0.0",
    "@oada/types": "^3.5.3",
    "@overleaf/o-error": "^3.4.0",
    "@types/json-pointer": "^1.0.34",
    "bluebird": "^3.7.2",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "cron": "^4.3.3",
    "csvjson": "^5.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "debug": "^4.4.3",
    "deep-equal": "^2.2.3",
    "dotenv": "^17.2.3",
    "es-main": "^1.4.0",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.4",
    "fuse.js": "^7.1.0",
    "js-sha256": "^0.11.1",
    "json-pointer": "^0.6.2",
    "json-ptr": "^3.1.1",
    "jsonpath": "^1.1.1",
    "jszip": "^3.10.1",
    "ksuid": "^3.0.0",
    "ky": "^1.11.0",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "mssql": "^11.0.1",
    "serialize-error": "^12.0.0",
    "sha256": "^0.2.0",
    "tslib": "^2.8.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@biomejs/biome": "^2.2.5",
    "@tsconfig/node22": "^22.0.2",
    "@types/bluebird": "^3.5.42",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/cron": "^2.4.3",
    "@types/debug": "^4.1.12",
    "@types/deep-equal": "^1.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.6.2",
    "@yarnpkg/sdks": "^3.2.3",
    "ava": "^6.4.1",
    "bluebird": "^3.7.2",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "chai-as-promised": "^8.0.2",
    "typescript": "5.9.3"
  },
  "resolutions": {
    "@oada/lib-prom": "^4.0.1",
    "cross-spawn": "^7.0.5",
    "jsonpath-plus": "^10.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "packageManager": "yarn@4.10.3",
  "volta": {
    "node": "22.9.0"
  }
}
