{
  "name": "@qlever-llc/fl-sync",
  "version": "2.0.0",
  "description": "A Trellis microservice to sync Food Logiq data to a Trellis cloud",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "type": "module",
  "author": "Sam Noel <sam@qlever.io>",
  "license": "Apache-2.0",
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^4.5.0",
    "@oada/jobs": "^4.2.1",
    "@oada/lib-prom": "^3.8.0",
    "@oada/list-lib": "4.2.6",
    "@oada/pino-debug": "^3.9.1",
    "@oada/poll": "^1.0.4",
    "@oada/types": "^3.4.5",
    "@overleaf/o-error": "^3.4.0",
    "@types/json-pointer": "^1.0.31",
    "bluebird": "^3.7.2",
    "clone-deep": "^4.0.1",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "cron": "^2.3.0",
    "csvjson": "^5.1.0",
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "es-main": "^1.2.0",
    "fast-deep-equal": "^3.1.3",
    "got": "^12.6.0",
    "js-sha256": "^0.9.0",
    "json-pointer": "^0.6.2",
    "jszip": "^3.10.1",
    "ksuid": "^3.0.0",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.set": "^4.3.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mssql": "^9.1.1",
    "sha256": "^0.2.0",
    "tslib": "^2.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/bluebird": "^3.5.38",
    "@types/clone-deep": "^4",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/cron": "^2.0.1",
    "@types/debug": "^4.1.7",
    "@types/lodash.find": "^4",
    "@types/lodash.findindex": "^4",
    "@types/lodash.get": "^4",
    "@types/lodash.has": "^4",
    "@types/lodash.set": "^4",
    "@types/md5": "^2.3.2",
    "@types/mssql": "^8.1.2",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@yarnpkg/sdks": "^3.0.0-rc.42",
    "ava": "^5.2.0",
    "bluebird": "^3.7.2",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.27.5",
